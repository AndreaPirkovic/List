package pirkovic.andrea;
import java.io.*;
import java.util.*;
	 
public class Main {
	//Kreiranje liste upotrebom klase Stack
	 
	 public static void main(String[] args)
	    {
	        
	 
	        // Deklaracija stack-a
		 	Stack<String> stack = new Stack<>();

		 	stack.push("kurs");
		 	stack.push("JAVA");
		 	stack.push("programiranja");
	 
	        
	         
	        //Stampa 
	        System.out.println(stack);
	 
	        //Uzimanje elementa sa vrha stack-a
	        stack.pop();
	 
	        
	 
	        // Stampa elemenata liste
	        for (int i = 0; i < stack.size(); i++)
	            System.out.print(stack.get(i) + " ");
	    }
	}

